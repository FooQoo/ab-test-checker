{
  "name": "abtest-checker",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap --config sitemap.config.js",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "test": "run-p -l -c --aggregate-output lint:* unit",
    "unit": "jest",
    "prepare": "husky install",
    "lint:css": "stylelint src/**/*.{css,scss,sass}"
  },
  "dependencies": {
    "@apollo/client": "3.4.5",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "@reduxjs/toolkit": "^1.6.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/postcss7-compat": "^2.2.7",
    "chi-sq-test": "^1.1.0",
    "graphql": "15.5.1",
    "hamburger-react": "^2.4.1",
    "modern-css-reset": "^1.4.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-medium-image-zoom": "4.3.4",
    "react-redux": "^7.2.4",
    "redux-logger": "^3.0.6",
    "sass": "^1.38.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "devDependencies": {
    "@iconify/react": "^3.0.0",
    "@testing-library/react": "12.0.0",
    "@types/gtag.js": "0.0.7",
    "@types/jest": "26.0.24",
    "@types/node": "16.0.0",
    "@types/react": "17.0.16",
    "@types/react-redux": "^7.1.18",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "autoprefixer": "^9",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.5",
    "babel-plugin-macros": "3.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.1.2",
    "next-sitemap": "1.6.148",
    "npm-run-all": "4.1.5",
    "postcss": "^7",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "stylelint-scss": "3.20.1",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn fix:eslint",
    "*": "yarn fix:prettier"
  }
}
